{
    "name": "stelle-music",
    "version": "0.3.4.1-BLAZER",
    "description": "A music bot.",
    "main": "./dist/index.js",
    "type": "module",
    "packageManager": "pnpm@10.7.1+sha512.2d92c86b7928dc8284f53494fb4201f983da65f0fb4f0d40baafa5cf628fa31dae3e5968f12466f17df7e97310e30f343a648baea1b9b350685dafafffdf5808",
    "homepage": "https://github.com/Ganyu-Studios/stelle-music#readme",
    "keywords": [
        "discord",
        "discord.js",
        "seyfert",
        "music",
        "bot",
        "discord bot",
        "stelle",
        "lavalink",
        "lavalink-client"
    ],
    "author": "Ganyu Studios",
    "license": "MIT",
    "contributors": [
        {
            "name": "JustEvil",
            "url": "https://github.com/EvilG-MC"
        }
    ],
    "engines": {
        "node": ">= 22.11.0",
        "pnpm": ">= 10.7.0"
    },
    "scripts": {
        "build": "tsc",
        "typecheck": "tsc --noEmit",
        "clean": "node ./scripts/clean.js && pnpm build",
        "start": "node ./dist/index.js",
        "dev": "tsx ./src/index.ts --debug --dev",
        "lint": "biome lint --write .",
        "format": "biome check --write .",
        "generate": "prisma generate",
        "push": "prisma db push",
        "prepare": "husky"
    },
    "lint-staged": {
        "{*.ts,*.json}": ["pnpm format"]
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/Ganyu-Studios/stelle-music.git"
    },
    "bugs": {
        "url": "https://github.com/Ganyu-Studios/stelle-music/issues"
    },
    "dependencies": {
        "@prisma/client": "^6.5.0",
        "dotenv": "^16.4.7",
        "ioredis": "^5.6.0",
        "lavalink-client": "^2.5.0",
        "meowdb": "^2.2.3",
        "seyfert": "github:tiramisulabs/seyfert",
        "yunaforseyfert": "^1.1.0"
    },
    "devDependencies": {
        "@biomejs/biome": "^1.9.4",
        "@types/node": "^22.14.0",
        "husky": "^9.1.7",
        "lint-staged": "^15.5.0",
        "prisma": "^6.5.0",
        "tsx": "^4.19.3",
        "typescript": "^5.8.3"
    },
    "imports": {
        "#stelle/classes/*": "./dist/structures/classes/*",
        "#stelle/utils/*": "./dist/structures/utils/*",
        "#stelle/types": "./dist/structures/types/index.js",
        "#stelle/middlewares": "./dist/middlewares/index.js"
    },
    "pnpm": {
        "onlyBuiltDependencies": [
            "@biomejs/biome",
            "@prisma/client",
            "@prisma/engines",
            "esbuild",
            "prisma"
        ]
    }
}
