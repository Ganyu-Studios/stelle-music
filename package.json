{
    "name": "stelle-music",
    "version": "0.3.4.1-BLAZER",
    "description": "A music bot.",
    "main": "./dist/index.js",
    "type": "module",
    "packageManager": "pnpm@10.9.0+sha512.0486e394640d3c1fb3c9d43d49cf92879ff74f8516959c235308f5a8f62e2e19528a65cdc2a3058f587cde71eba3d5b56327c8c33a97e4c4051ca48a10ca2d5f",
    "homepage": "https://github.com/Ganyu-Studios/stelle-music#readme",
    "keywords": [
        "discord",
        "discord.js",
        "seyfert",
        "music",
        "bot",
        "discord bot",
        "stelle",
        "lavalink",
        "lavalink-client"
    ],
    "author": "Ganyu Studios",
    "license": "MIT",
    "contributors": [
        {
            "name": "JustEvil",
            "url": "https://github.com/EvilG-MC"
        }
    ],
    "engines": {
        "node": ">= 22.11.0",
        "pnpm": ">= 10.7.0"
    },
    "scripts": {
        "build": "tsc",
        "typecheck": "tsc --noEmit",
        "clean": "node ./scripts/clean.js && pnpm build",
        "start": "node ./dist/index.js",
        "dev": "tsx ./src/index.ts --debug --dev",
        "lint": "biome lint --write .",
        "format": "biome check --write .",
        "generate": "prisma generate",
        "push": "prisma db push",
        "prepare": "husky"
    },
    "lint-staged": {
        "{*.ts,*.json}": ["pnpm format"]
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/Ganyu-Studios/stelle-music.git"
    },
    "bugs": {
        "url": "https://github.com/Ganyu-Studios/stelle-music/issues"
    },
    "dependencies": {
        "@prisma/client": "^6.6.0",
        "dotenv": "^16.5.0",
        "ioredis": "^5.6.1",
        "lavalink-client": "^2.5.0",
        "meowdb": "^2.2.3",
        "seyfert": "github:tiramisulabs/seyfert",
        "yunaforseyfert": "^1.1.0"
    },
    "devDependencies": {
        "@biomejs/biome": "^1.9.4",
        "@types/node": "^22.15.2",
        "husky": "^9.1.7",
        "lint-staged": "^15.5.1",
        "prisma": "^6.6.0",
        "tsx": "^4.19.3",
        "typescript": "^5.8.3"
    },
    "imports": {
        "#stelle/classes/*": "./dist/structures/classes/*",
        "#stelle/utils/*": "./dist/structures/utils/*",
        "#stelle/types": "./dist/structures/types/index.js",
        "#stelle/middlewares": "./dist/middlewares/index.js",
        "#stelle/client": "./dist/index.js",
        "#stelle/prisma": "./generated/prisma/index.js"
    },
    "pnpm": {
        "onlyBuiltDependencies": [
            "@biomejs/biome",
            "@prisma/client",
            "@prisma/engines",
            "esbuild",
            "prisma"
        ]
    }
}
