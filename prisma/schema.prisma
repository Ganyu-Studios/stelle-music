generator client {
  provider = "prisma-client-js"
  output   = "../generated/prisma"
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

model guildLocale {
  id     String @id @map("_id")
  locale String @default("en-US")
}

model guildPrefix {
  id     String @id @map("_id")
  prefix String
}

model guildPlayer {
  id             String @id @map("_id")
  searchPlatform String @default("spsearch")
  defaultVolume  Int    @default(100)
}

model guildRequest {
  id        String @id @map("_id")
  channelId String
  messageId String?
}
